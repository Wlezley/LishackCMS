  "scripts": {
    "build": "webpack --config webpack.prod.js --env build=prod",
    "build:dev": "webpack --watch --config webpack.dev.js --env build=dev",
    "build:admin": "webpack --config webpack.admin.js",
    "build:admin:dev": "webpack --watch --config webpack.admin.js --mode development",
    "build:tinymce": "webpack --config webpack.tinymce.js",
    "build:tinymce:dev": "webpack --watch --config webpack.tinymce.js --mode development",
    "build:all": "npm run build && npm run build:admin && npm run build:tinymce",
    "build:all:dev": "webpack --config webpack.dev.js && webpack --config webpack.admin.js --mode development && webpack --config webpack.tinymce.js --mode development",
    "drop:dist": "webpack --config webpack.drop.dist.js",
    "drop:cache": "webpack --config webpack.drop.cache.js"
  }
