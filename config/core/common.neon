parameters:
    debugMode: 'auto'
    strictMode: false

application:
    errorPresenter: Error
    mapping:
        *: App\Modules\*\Presenters\*Presenter

database:
    user: %database.user%
    password: %database.password%
    dsn: "mysql:host=%database.host%;dbname=%database.name%"
    options:
        lazy: true


session:
    expiration: 14 days


di:
    export:
        parameters: yes
        tags: no
